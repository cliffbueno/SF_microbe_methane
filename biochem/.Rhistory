geom_text(data = coords1, aes(x = longitude, y = latitude - 0.02, label = Site),
colour = "black", size = 4, fontface = "bold") +
geom_text(data = coords2, aes(x = longitude, y = latitude + 0.02, label = Site),
colour = "black", size = 4, fontface = "bold") +
geom_text(aes(x = -121.7, y = 38.2, label = "Delta"),
colour = "black", size = 6, fontface = "italic") +
geom_text(aes(x = -122.4, y = 38.05, label = "San Pablo\nBay"),
colour = "white", size = 3) +
geom_text(aes(x = -122.55, y = 37.79, label = "Pacific\nOcean"),
colour = "white", size = 3) +
geom_text(aes(x = -122.45, y = 37.75, label = "San Francisco"),
colour = "grey40", size = 3) +
geom_text(aes(x = -122.27, y = 37.81, label = "Oakland"),
colour = "grey40", size = 3) +
geom_path(data = path, aes(x = x, y = y), linetype = "dashed", size = 0.25) +
scale_colour_manual(values = site_colors$color) +
xlab("Longitude (°W)") +
ylab("Latitude (°N)") +
scalebar(x.min = -122.6,
y.min = 37.76,
x.max = -121.6,
y.max = 38.26,
dist = 10, dist_unit = "km", height = 0.03, st.dist = 0.03, st.size = 4,
transform = TRUE, model = "WGS84", location = "bottomright") +
theme(legend.position = "none",
plot.margin = unit(c(0.1,0.6,0.1,0.1), "cm"))
b
north2(b, x = 0.94, y = 0.15, scale = 0.1)
